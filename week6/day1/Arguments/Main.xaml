  <Flowchart DisplayName="Find perfect square" sap2010:WorkflowViewState.IdRef="Flowchart_2" StartNode="{x:Reference __ReferenceID5}">
    <FlowStep x:Name="__ReferenceID5" sap2010:WorkflowViewState.IdRef="FlowStep_5">
      <Flowchart DisplayName="Flow Chart Check if given year is a leap year" sap2010:WorkflowViewState.IdRef="Flowchart_1">
        <Flowchart.Variables>
          <Variable x:TypeArguments="x:Int32" Name="userYear" />
          <Variable x:TypeArguments="x:Double" Name="yearRemainder" />
        </Flowchart.Variables>
        <Flowchart.StartNode>
          <x:Reference>__ReferenceID0</x:Reference>
        </Flowchart.StartNode>
        <FlowStep x:Name="__ReferenceID0" sap2010:WorkflowViewState.IdRef="FlowStep_4">
          <ui:InputDialog Options="{x:Null}" DisplayName="Take user input for year" sap2010:WorkflowViewState.IdRef="InputDialog_1" IsPassword="False" Label="Year" Title="Check if input year is a leap year.">
            <ui:InputDialog.Result>
              <OutArgument x:TypeArguments="x:Int32">[userYear]</OutArgument>
            </ui:InputDialog.Result>
          </ui:InputDialog>
          <FlowStep.Next>
            <FlowStep x:Name="__ReferenceID2" sap2010:WorkflowViewState.IdRef="FlowStep_3">
              <Assign DisplayName="Assigns temp value of year mod 4" sap2010:WorkflowViewState.IdRef="Assign_1">
                <Assign.To>
                  <OutArgument x:TypeArguments="x:Double">[yearRemainder]</OutArgument>
                </Assign.To>
                <Assign.Value>
                  <InArgument x:TypeArguments="x:Double">[userYear mod 4]</InArgument>
                </Assign.Value>
              </Assign>
              <FlowStep.Next>
                <FlowDecision x:Name="__ReferenceID1" Condition="[yearRemainder = 0]" DisplayName="If yearRemainder is 0 or not" sap2010:WorkflowViewState.IdRef="FlowDecision_1">
                  <FlowDecision.True>
                    <FlowStep x:Name="__ReferenceID3" sap2010:WorkflowViewState.IdRef="FlowStep_1">
                      <ui:MessageBox ChosenButton="{x:Null}" Buttons="Ok" Caption="Confirmed a leap year" DisplayName="Is a leap year" sap2010:WorkflowViewState.IdRef="MessageBox_1" Text="[userYear.ToString + &quot; is a leap year&quot;]" TopMost="True" />
                    </FlowStep>
                  </FlowDecision.True>
                  <FlowDecision.False>
                    <FlowStep x:Name="__ReferenceID4" sap2010:WorkflowViewState.IdRef="FlowStep_2">
                      <ui:MessageBox ChosenButton="{x:Null}" Buttons="Ok" Caption="Confirmed not a leap year" DisplayName="Not a leap year" sap2010:WorkflowViewState.IdRef="MessageBox_2" Text="[userYear.ToString + &quot; is not a leap year&quot;]" TopMost="True" />
                      <FlowStep.Next>
                        <x:Reference>__ReferenceID0</x:Reference>
                      </FlowStep.Next>
                    </FlowStep>
                  </FlowDecision.False>
                </FlowDecision>
              </FlowStep.Next>
            </FlowStep>
          </FlowStep.Next>
        </FlowStep>
        <x:Reference>__ReferenceID1</x:Reference>
        <x:Reference>__ReferenceID2</x:Reference>
        <x:Reference>__ReferenceID3</x:Reference>
        <x:Reference>__ReferenceID4</x:Reference>
      </Flowchart>
    </FlowStep>
