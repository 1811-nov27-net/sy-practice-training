  <Flowchart DisplayName="Compute sum of n natural numbers flowchart" sap2010:WorkflowViewState.IdRef="Flowchart_1">
    <Flowchart.Variables>
      <Variable x:TypeArguments="x:Int32" Name="totalNumbers" />
      <Variable x:TypeArguments="x:Int32" Default="0" Name="sum" />
      <Variable x:TypeArguments="x:Int32" Default="0" Name="startLoop" />
    </Flowchart.Variables>
    <Flowchart.StartNode>
      <FlowStep x:Name="__ReferenceID1" sap2010:WorkflowViewState.IdRef="FlowStep_1">
        <ui:InputDialog Options="{x:Null}" DisplayName="Ask user for n input" sap2010:WorkflowViewState.IdRef="InputDialog_1" IsPassword="False" Label="How many natural numbers do you want to compute the sum of?" Title="Assigning n numbers">
          <ui:InputDialog.Result>
            <OutArgument x:TypeArguments="x:Int32">[totalNumbers]</OutArgument>
          </ui:InputDialog.Result>
        </ui:InputDialog>
        <FlowStep.Next>
          <FlowDecision x:Name="__ReferenceID0" Condition="[startLoop &gt; totalNumbers]" DisplayName="Flow Decision" sap2010:WorkflowViewState.IdRef="FlowDecision_1">
            <FlowDecision.True>
              <FlowStep x:Name="__ReferenceID4" sap2010:WorkflowViewState.IdRef="FlowStep_4">
                <ui:MessageBox ChosenButton="{x:Null}" Buttons="Ok" Caption="Sum of n numbers" DisplayName="Total Sum" sap2010:WorkflowViewState.IdRef="MessageBox_1" Text="[&quot;The sum is: &quot; + sum.ToString]" TopMost="True" />
              </FlowStep>
            </FlowDecision.True>
            <FlowDecision.False>
              <FlowStep x:Name="__ReferenceID2" sap2010:WorkflowViewState.IdRef="FlowStep_3">
                <Assign sap2010:WorkflowViewState.IdRef="Assign_1">
                  <Assign.To>
                    <OutArgument x:TypeArguments="x:Int32">[sum]</OutArgument>
                  </Assign.To>
                  <Assign.Value>
                    <InArgument x:TypeArguments="x:Int32">[sum + startLoop]</InArgument>
                  </Assign.Value>
                </Assign>
                <FlowStep.Next>
                  <FlowStep x:Name="__ReferenceID3" sap2010:WorkflowViewState.IdRef="FlowStep_2">
                    <Assign sap2010:WorkflowViewState.IdRef="Assign_2">
                      <Assign.To>
                        <OutArgument x:TypeArguments="x:Int32">[startLoop]</OutArgument>
                      </Assign.To>
                      <Assign.Value>
                        <InArgument x:TypeArguments="x:Int32">[startLoop + 1]</InArgument>
                      </Assign.Value>
                    </Assign>
                    <FlowStep.Next>
                      <x:Reference>__ReferenceID0</x:Reference>
                    </FlowStep.Next>
                  </FlowStep>
                </FlowStep.Next>
              </FlowStep>
            </FlowDecision.False>
          </FlowDecision>
        </FlowStep.Next>
      </FlowStep>
    </Flowchart.StartNode>
